	abstract = "{The limiting distribution of Yang-Lee zeros in a complex field plane is considered for general one-dimensional classical systems with interactions of finite range. On the basis of a simple assumption, it is shown that behavior near the Yang-Lee edge, the terminus of the zero distribution, is universal, the “protocritical” exponents taking the values σ= −½, ϒc = 1½, νc=½ and η = −1; likewise the scaling function for the pair correlations is universal. The underlying assumption is confirmed in numerical studies of m × ∞ spin ½ Ising strips (or cylinders) and of n-vector chains for general n. The crossovers occurring when m→∞ and n→∞ are considered briefly.}",
	issn = {0375-9687},
	doi = {10.1143/PTP.69.14},
	url = {https://doi.org/10.1143/PTP.69.14},
}

